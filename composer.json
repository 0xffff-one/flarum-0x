    "require": {
        "askvortsov/flarum-pwa": "*",
        "blomstra/flarum-redis": "*",
        "flarum-lang/chinese-simplified": "^1.0",
        "flarum/approval": "*",
        "flarum/bbcode": "*",
        "flarum/core": "^1.0",
        "flarum/emoji": "*",
        "flarum/flags": "*",
        "flarum/lang-english": "*",
        "flarum/likes": "*",
        "flarum/lock": "*",
        "flarum/markdown": "*",
        "flarum/mentions": "*",
        "flarum/nicknames": "*",
        "flarum/pusher": "*",
        "flarum/statistics": "*",
        "flarum/sticky": "*",
        "flarum/subscriptions": "*",
        "flarum/suspend": "*",
        "flarum/tags": "*",
        "fof/links": "*",
        "fof/nightmode": "*",
        "fof/oauth": "*",
        "fof/pages": "^1.0",
        "fof/sitemap": "*",
        "fof/socialprofile": "*",
        "fof/split": "*",
        "fof/upload": "*",
        "fof/user-bio": "*",
        "fof/webhooks": "*",
        "ganuonglachanh/sonic": "^0.1.2",
        "ianm/syndication": "*",
        "league/flysystem-aws-s3-v3": "1.*",
        "minishlink/web-push": "*",
        "overtrue/pinyin": "~4.0",
        "phenomlab/fancybox-wrapper": "^0.8.0",
        "the-turk/flarum-mathren": "^0.3.4",
        "v17development/flarum-seo": "*"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "apcu-autoloader": true
    },
    "minimum-stability": "beta",
    "prefer-stable": true,
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://mirrors.aliyun.com/composer/"
        }
    }
